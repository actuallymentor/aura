{
  "expo": {
    "name": "Oura Trend Dashboard",
    "description": "A trend dashboard for Oura ring data.",
    "slug": "oura-trend-dashboard",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mentorpalokaj.ouratrenddashboard",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mentorpalokaj.ouratrenddashboard",
      "versionCode": 1,
      "permissions" : []
    },
    "scheme": "ouratrenddashboard",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
        }
      ]
    }
  }
}